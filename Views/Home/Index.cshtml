@using E_Shelf_WebUI.ResponseModel
@model List<ProductResponseModel>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>E Shelf WebApi Consume</title>
</head>
<body>
    <p>
        UI Layer Working..
    </p>

    <table border="1">
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Stock</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Stock</td>
            </tr>
        }
    </table>
</body>
</html>